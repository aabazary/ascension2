{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/((?!assets/|mages/|dragons/|dragonling/|armor/|spells/|ascenion/|screenshots/).*)",
      "dest": "/index.html"
    }
  ]
}
